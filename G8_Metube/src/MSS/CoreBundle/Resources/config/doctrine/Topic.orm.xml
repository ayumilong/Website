<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MSS\CoreBundle\Entity\Topic" table="topic">
    <id name="topicid" type="integer" column="topicid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="topicname" type="string" column="topicname" length="20" nullable="false"/>
    <field name="description" type="string" column="description" length="40" nullable="true"/>
    <field name="createtime" type="date" column="createtime" nullable="false"/>
  </entity>
</doctrine-mapping>
