<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MSS\CoreBundle\Entity\Contactlist" table="contactlist">
    <indexes>
      <index name="fromname" columns="fromname"/>
      <index name="toname" columns="toname"/>
    </indexes>
    <id name="contactid" type="integer" column="contactid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="isfriend" type="integer" column="isfriend" nullable="true"/>
    <field name="block" type="integer" column="block" nullable="true"/>
    <field name="blocktype" type="integer" column="blocktype" nullable="true"/>
    <field name="blockcontent" type="string" column="blockcontent" length="100" nullable="true"/>
    <many-to-one field="toname" target-entity="User">
      <join-columns>
        <join-column name="toname" referenced-column-name="username"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fromname" target-entity="User">
      <join-columns>
        <join-column name="fromname" referenced-column-name="username"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
